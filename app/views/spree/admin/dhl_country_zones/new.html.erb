<% content_for :page_title do %>
  <%= Spree.t(:new_country) %>
<% end %>

<% content_for :page_actions do %>
  <%= button_link_to Spree.t(:back), collection_url, icon: 'arrow-left' %>
<% end %>

<%= render partial: 'spree/shared/error_messages', locals: { target: @dhl_country_zone } %>

<%= form_for [:admin, @dhl_shipping_zone, @dhl_country_zone].compact do |f| %>
  <fieldset class="no-border-top">
    <%= render partial: 'form', locals: { f: f } %>
    <%= render partial: 'spree/admin/shared/new_resource_links' %>
  </fieldset>
<% end %>
